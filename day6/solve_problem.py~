INPUT = "sample.txt"
#INPUT = "input.txt"
RE_PTRN1 = "\W+"


from re import split as re_split




def main():
    raw = load_file(INPUT)
    time, dist = parse_raw(raw)

    
    print(time)
    print(dist)

    
def load_file(f):
    with open(f) as f:
        data = f.readlines()
    return data

def parse_raw(data):
    return list(map(lambda text: re_split(RE_PTRN1, text), data))


if __name__ == "__main__":
    main()
